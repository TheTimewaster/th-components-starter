{
  "name": "@th-components/lib",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "files": [
    "dist/{scss}/**.scss"
  ],
  "module": "./dist/th-components.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/th-components.es.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "build": "vite build && yarn build-types",
    "build-types": "vue-tsc --declaration --emitDeclarationOnly -p tsconfig.build.json",
    "build-only": "vite build",
    "test": "vitest",
    "type-check": "vue-tsc --noEmit -p tsconfig.lib.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path ../../.gitignore"
  },
  "dependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@babel/types": "^7.20.5",
    "sass": "^1.56.1",
    "vite-plugin-static-copy": "^1.0.1"
  }
}
