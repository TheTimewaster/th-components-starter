{
  "name": "@th-components/lib",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "files": [
    "dist/{scss}/**.scss"
  ],
  "module": "./dist/index.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "types": "./dist/types/index.d.ts"
    },
    "./assets/*": {
      "import": "./dist/assets/*",
      "require": "./dist/assets/*"
    }
  },
  "scripts": {
    "build": "vite build",
    "build-types": "vue-tsc --project tsconfig.build.json && resolve-tspaths --project tsconfig.build.json --out dist/types",
    "build-only": "vite build",
    "test": "vitest",
    "type-check": "vue-tsc --noEmit -p tsconfig.lib.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path ../../.gitignore",
    "list:build": "tsc tools/list-exported.ts --target esnext --moduleResolution NodeNext --module NodeNext",
    "list:run": "node tools/list-exported.js",
    "list:lint": "eslint --fix ./constants/*.ts",
    "list": "yarn list:build && yarn list:run && yarn list:lint"
  },
  "dependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@babel/types": "^7.20.5",
    "consola": "^3.2.3",
    "resolve-tspaths": "^0.8.18",
    "sass": "^1.56.1",
    "vite-plugin-dts": "^3.7.3",
    "vite-plugin-static-copy": "^1.0.1"
  }
}
