{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@composables/*": ["src/composables/*"]
    },
    "declarationDir": "dist/types",
    "declaration": true,
    "sourceMap": true,
  },
  "include": ["./src/**/*.ts", "./src/**/*.vue"],
}
