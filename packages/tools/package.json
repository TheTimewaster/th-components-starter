{
  "name": "@th-components/tools",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "list:build": "tsc --project tsconfig.json",
